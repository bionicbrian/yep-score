{
  "name": "yep-poll",
  "version": "1.0.0",
  "description": "Let the people be heard. Built with yep-app.",
  "main": "index.js",
  "scripts": {
    "server": "node index.js",
    "css": "sass lib/styles:public/css",
    "css:watch": "sass --watch lib/styles:public/css",
    "compile": "browserify ./lib/main.js -o ./public/js/main.js -t babelify",
    "compile:watch": "watchify lib/main.js -o public/js/main.js -v -t babelify",
    "start": "npm run compile:watch && npm run css:watch && npm run server"
  },
  "author": "Brian Moore",
  "license": "ISC",
  "dependencies": {
    "@yuzu/yep-app": "^0.21.1",
    "babel-core": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "watchify": "^3.7.0"
  }
}
